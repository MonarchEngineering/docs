{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Airtop",
  "colors": {
    "primary": "#0891B2",
    "light": "#0891B2",
    "dark": "#0891B2"
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Getting started",
        "pages": [
          "index",
          "quickstart"
        ]
      },
      {
        "group": "Guides",
        "pages": [
          "essentials/settings",
          "essentials/navigation"
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Blog",
          "href": "https://www.airtop.ai/blog",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://discord.com",
          "icon": "discord"
        },
        {
          "anchor": "Changelog",
          "href": "https://airtop.ai/changelog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/logo.svg",
    "dark": "/logo/logo.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://portal.airtop.ai"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity",
     "mcp",
     "cursor",
     "vscode"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/",
      "github": "https://github.com/",
      "linkedin": "https://linkedin.com/"
    }
  }
}
