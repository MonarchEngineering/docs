{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Airtop",
  "colors": {
    "primary": "#0891B2",
    "light": "#0891B2",
    "dark": "#0891B2"
  },
  "appearance": {
    "default": "light",
    "strict": true
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "groups": [
      {
        "group": "Guides",
        "pages": [
          "guides/introduction",
          "guides/quick-start",
          "guides/authentication",
          "guides/bot-detection"
        ]
      },
      {
        "group": "Airtop Account",
        "pages": [
          "billing/*"
        ]
      },
      {
        "group": "Connections",
        "pages": [
          "connections/make",
          "connections/n8n",
          "connections/zapier"
        ]
      },
      {
        "group": "Agent API",
        "openapi": "openapi.json"
      }
    ]
  },
  "global": {
      "anchors": []
    },
  "logo": {
    "light": "/logo/logo.svg",
    "dark": "/logo/logo-dark.svg",
    "href": "https://www.airtop.ai"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://portal.airtop.ai"
    }
  },
  "contextual": {
    "options": [
     "copy",
     "view",
     "chatgpt",
     "claude",
     "perplexity"
   ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/airtop_ai",
      "linkedin": "https://www.linkedin.com/company/airtop-ai"
    }
  }
}
